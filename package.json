{
  "name": "data-table",
  "version": "1.0.0",
  "description": "Data Table allows you to create and manage tables on your website.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "start": "webpack --mode=development --watch",
    "sync": "browser-sync start -p 'http://localhost/' --files '**/*.php' 'dist/*.js' 'dist/*.css'",
    "preview": "npm-run-all --parallel sync start",
    "i18n-pot": "wp i18n make-pot . languages/test-project.pot --exclude=\"src\"",
    "cp": "./languages/test-project.pot ./languages/test-project-en_US.po",
    "i18n-json": "wp i18n make-json languages/ --no-purge",
    "i18n-mo": "wp i18n make-mo languages/",
    "i18n": "npm run i18n-pot && npm run i18n-json && npm run i18n-mo",
    "build": "gulp clean && npm run lint && webpack --mode=production && npm run i18n && gulp bundle"
  },
  "author": "Wasek Billah",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-react": "^7.24.1",
    "@wordpress/blob": "^3.54.0",
    "@wordpress/block-editor": "^12.22.0",
    "@wordpress/blocks": "^12.31.0",
    "@wordpress/components": "^27.2.0",
    "@wordpress/compose": "^6.31.0",
    "@wordpress/data": "^9.24.0",
    "@wordpress/element": "^5.31.0",
    "@wordpress/html-entities": "^3.54.0",
    "@wordpress/i18n": "^4.54.0",
    "@wordpress/rich-text": "^6.31.0",
    "@wordpress/server-side-render": "^4.31.0",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "browser-sync": "^3.0.2",
    "browserslist": "^4.23.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.10.0",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "del": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-node": "^4.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^4.1.0",
    "file-loader": "^6.2.0",
    "gulp": "^4.0.2",
    "gulp-zip": "^6.0.0",
    "mini-css-extract-plugin": "^2.8.1",
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^8.1.1",
    "react-inlinesvg": "^4.1.3",
    "sass-loader": "^14.1.1",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@tinymce/tinymce-react": "^5.0.0",
    "bpl-gutenberg-panel": "^0.0.25",
    "immer": "^10.0.4",
    "react-sortable-hoc": "^2.0.0"
  }
}
